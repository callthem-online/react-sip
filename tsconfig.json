{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "strictNullChecks": true,
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "allowSyntheticDefaultImports": false,
    "pretty": true,
    "removeComments": true,
    "lib": ["es6", "esnext.asynciterable", "dom"]
  },
  "exclude": ["build/**/*", "node_modules"]
}
